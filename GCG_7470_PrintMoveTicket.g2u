			V.Local.sScrapDescription.Set(V.ODBC.Con!rstSelA.FieldValTrim!SCRAP_CODE_DESC)
		F.Intrinsic.Control.EndIf
		F.ODBC.Con!rstSelA.Close

		F.Intrinsic.String.Build("SELECT * FROM GCG_7470_mv_tckt_lbl WHERE TRMNL = '{0}' ",V.Caller.Terminal,V.Global.sSQL)
