Program.Sub.SendEmail.Start
V.Local.sError.Declare
V.Local.iUserID.Declare
V.Local.sBody.Declare
V.Local.sSubject.Declare
V.Local.sRecip.Declare
V.Local.sSender.Declare
V.Local.sSQL.Declare
V.Local.sBodyHTML.Declare
V.Local.sJob.Declare
V.Local.sSuffix.Declare
V.Local.sSeq.Declare
F.Intrinsic.Control.Try
'Get user id
F.Global.Security.GetUserId(V.Caller.User,V.Caller.CompanyCode,V.Local.iUserID)

F.ODBC.Connection!conx.OpenCompanyConnection
'Get sender email from registry
F.Global.Registry.ReadValue("SUPERVSR",V.Caller.CompanyCode,"Sender",7470,1000,5,"",V.Local.sSender)
'Pull datatable info from custom table
F.Intrinsic.Control.If(V.Args.Status,=,"Scrap")
	F.Intrinsic.String.Build("SELECT JOB, SUFFIX, CUR_SEQ, PART, WORKCENTER AS WC, CUR_DESCR AS DESCR, SCRAP_QTY, EMPLOYEE, NEXT_PART AS NEXT_WC, NEXT_DESCR from GCG_7470_MV_TCKT_LBL WHERE TRMNL = '{0}'",V.Caller.Terminal,V.Local.sSQL)
	F.Data.Datatable.CreateFromSQL("dtExport","conx",V.Local.sSQL,True)
	'Export body to html
	F.Data.Datatable.ExportHTML("dtExport",V.DataTable.dtExport.FieldNames,"Job*!*Suffix*!*Seq*!*Part*!*WC*!*Desc*!*Quantity*!*Employee*!*Next WC/Op*!*Next Desc","",False,V.Local.sBodyHTML)
	F.Intrinsic.String.Replace(V.Local.sBodyHTML,"<table>","<table style="width:75%">",V.Local.sBodyHTML)
	F.Intrinsic.String.Replace(V.Local.sBodyHTML,"<th>","<th ALIGN=LEFT>",V.Local.sBodyHTML)
F.Intrinsic.Control.Else
	F.Intrinsic.String.Build("SELECT JOB, SUFFIX, CUR_SEQ, PART, WORKCENTER AS WC, CUR_DESCR AS DESCR, QTY, EMPLOYEE, NEXT_PART AS NEXT_WC, NEXT_DESCR from GCG_7470_MV_TCKT_LBL WHERE TRMNL = '{0}'",V.Caller.Terminal,V.Local.sSQL)
	F.Data.Datatable.CreateFromSQL("dtExport","conx",V.Local.sSQL,True)
	'Export body to html
	F.Data.Datatable.ExportHTML("dtExport",V.DataTable.dtExport.FieldNames,"Job*!*Suffix*!*Seq*!*Part*!*WC*!*Desc*!*Quantity*!*Employee*!*Next WC/Op*!*Next Desc","",False,V.Local.sBodyHTML)
	F.Intrinsic.String.Replace(V.Local.sBodyHTML,"<table>","<table style="width:75%">",V.Local.sBodyHTML)
	F.Intrinsic.String.Replace(V.Local.sBodyHTML,"<th>","<th ALIGN=LEFT>",V.Local.sBodyHTML)
F.Intrinsic.Control.EndIf


F.ODBC.Connection!conx.Close

'Replace subject keywords
V.Local.sJob.Set(V.DataTable.dtExport(0).JOB!Fieldval)
V.Local.sSuffix.Set(V.DataTable.dtExport(0).SUFFIX!Fieldval)
V.Local.sSeq.Set(V.DataTable.dtExport(0).CUR_SEQ!Fieldval)

F.Data.Datatable.Close("dtExport")

V.Global.sSubject.Set("Move ticket for Job: #JOB# Suffix: #SUFFIX# Seq: #SEQ#")
F.Intrinsic.Control.If(V.Args.Status,=,"Scrap")
	F.Intrinsic.String.Replace(V.Global.sSubject,"Move","Scrap move",V.Global.sSubject)
F.Intrinsic.Control.EndIf
F.Intrinsic.String.Replace(V.Global.sSubject,"#JOB#",V.Local.sJob,V.Local.sSubject)
F.Intrinsic.String.Replace(V.Local.sSubject,"#SUFFIX#",V.Local.sSuffix,V.Local.sSubject)
F.Intrinsic.String.Replace(V.Local.sSubject,"#SEQ#",V.Local.sSeq,V.Local.sSubject)

F.Intrinsic.String.Replace(V.Global.sBody,"#BODY#",V.Local.sBodyHTML,V.Local.sBody)

'Get list of recipients
F.ODBC.Connection!conx.OpenCompanyConnection
F.ODBC.Connection!conx.ExecuteAndReturn("SELECT * FROM GCG_7470_EMAIL",V.Local.sRecip)
F.Intrinsic.String.Replace(V.Local.sRecip,"#$#","@!@",V.Local.sRecip)
F.ODBC.Connection!conx.Close

'Send email
F.Global.Messaging.QueueMessage(V.Caller.CompanyCode,V.Local.iUserID,"Move Ticket Printing (7470)",V.Local.sSubject,V.Local.sSender,V.Local.sRecip,"",5,"",False,"","","","","","","","","",V.Local.sBody)

F.Intrinsic.Control.Catch
	F.Intrinsic.String.Build("Project: {0}{1}{1}Subroutine: {2}{1}Line: {3}{1}Error Number: {4}{1}Description: {5}{1}{1}GAB Version: {6}",V.Caller.ScriptFile,V.Ambient.NewLine,V.Ambient.CurrentSubroutine,V.Ambient.ErrorLine,V.Ambient.ErrorNumber,V.Ambient.ErrorDescription,V.Ambient.GABVersion,V.Local.sError)
	F.Intrinsic.UI.Msgbox(V.Local.sError)
	F.Intrinsic.Control.End
F.Intrinsic.Control.EndTry
Program.Sub.SendEmail.End

