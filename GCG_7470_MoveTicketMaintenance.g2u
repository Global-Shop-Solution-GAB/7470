	F.Intrinsic.Control.If(V.Ambient.ErrorNumber,<>,0)
	
		F.Intrinsic.String.Concat("Project: GCG_7470_MoveTicketMaintenance.gas", V.Ambient.Newline, V.Ambient.Newline, "Subroutine: ", V.Ambient.CurrentSubroutine, V.Ambient.NewLine, "Error Occurred ", V.Ambient.ErrorNumber, " with description ", V.Ambient.ErrorDescription, V.Local.sError)
		F.Intrinsic.UI.Msgbox(V.Local.sError)
		F.Intrinsic.Control.CallSub(Form1_unload)
		
	F.Intrinsic.Control.EndIf

Program.Sub.cmdprnter_click.End

