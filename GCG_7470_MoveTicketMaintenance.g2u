				Gui.F_MoveTicket.txtPrinter.Text(v.Ambient.PRINTERDIALOGSELECTION)
		
			F.Intrinsic.Control.ElseIf(V.Args.CONTROLNAME,=,"CMDSCRAPPRNTR")
				Gui.F_MoveTicket.txtScrapPrinter.Text(v.Ambient.PRINTERDIALOGSELECTION)
		
