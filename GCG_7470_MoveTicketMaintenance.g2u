Program.Sub.Main.Start
	F.Intrinsic.Control.SetErrorHandler("Main_Err")
	F.Intrinsic.Control.ClearErrors
	
		V.Local.sError.Declare(String)
		
		'open ODBC connection
		F.ODBC.Connection!Con.OpenConnection(V.Ambient.PDSN,V.Ambient.PUser,V.Ambient.PPass)
		
		'look for existing records and load list view
		F.Intrinsic.Control.CallSub(Buildlistview)
		
		'show screen
		GUI.F_MoveTicket..Sizeable(False)
		GUI.F_MoveTicket..MaxButton(False)
		Gui.F_MoveTicket..Show
		
		F.Intrinsic.Control.ExitSub
		
	F.Intrinsic.Control.Label("Main_Err")
	F.Intrinsic.Control.If(V.Ambient.ErrorNumber,<>,0)
	
		F.Intrinsic.String.Concat("Project: GCG_7470_MoveTicketMaintenance.gas", V.Ambient.Newline, V.Ambient.Newline, "Subroutine: ", V.Ambient.CurrentSubroutine, V.Ambient.NewLine, "Error Occurred ", V.Ambient.ErrorNumber, " with description ", V.Ambient.ErrorDescription, V.Local.sError)
		F.Intrinsic.UI.Msgbox(V.Local.sError)
		F.Intrinsic.Control.CallSub(Form1_unload)
		
	F.Intrinsic.Control.EndIf

Program.Sub.Main.End
